{
    "name": "cntabana/inertia-vue",
    "description": "A laravel frontend command to turn your Controllers code to Vue Views",
    "version": "0.0.7",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruddy Ntabana",
            "email": "ntabanacoco@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^5.8"
    },
    "autoload": {
        "psr-4": {
            "InertiaVue\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "InertiaVue\\InertiaVueServiceProvider"
            ]
        }
    }
}
